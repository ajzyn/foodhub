model Product {
  id            String      @id @default(cuid())
  name          String
  category      Category
  description   String?
  price         Float
  bulkPrice     String?
  minOrder      Int
  leadTime      String?
  supplier   Supplier     @relation(fields: [supplierId], references: [id])
  supplierId String
  orderItems  OrderItem[]
  stock         Int
  certifications String?

  @@map("products")
}

enum Category {
  MEAT
  VEGETABLES
  DAIRY
  OTHER
}